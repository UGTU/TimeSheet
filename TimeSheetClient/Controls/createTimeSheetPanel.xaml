<UserControl x:Class="TimeSheetClient.createTimeSheetPanel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk" mc:Ignorable="d" d:DesignHeight="73" d:DesignWidth="258">
    
    <Grid x:Name="LayoutRoot" Background="{x:Null}" Width="255" Height="70">
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="3,10,0,0" Name="textBlock3" Text="Начало периода:" VerticalAlignment="Top" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="3,39,0,0" Name="textBlock4" Text="Конец периода:" VerticalAlignment="Top" />
        <sdk:DatePicker Height="23" HorizontalAlignment="Left" Margin="115,10,0,0" Name="dateBeginPeriod" VerticalAlignment="Top" Width="130" SelectedDateChanged="dateBeginPeriod_SelectedDateChanged" Loaded="dateBeginPeriod_Loaded" />
        <sdk:DatePicker Height="23" HorizontalAlignment="Left" Margin="115,39,0,0" Name="dateEndPeriod" VerticalAlignment="Top" Width="130" />
    </Grid>
</UserControl>
