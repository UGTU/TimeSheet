@model TimeSheetMvc4WebApplication.Models.PaperModel
@if (Model.IsLast)
{
    <table border="2" bordercolor="black" cellpadding="0" cellspacing="0" style="border-collapse: collapse; table-layout: fixed;">
        @Html.Partial("TimeSheetHeader", Model.HeaderStyle)
        @foreach (var employee in Model.Employees)
        {
            @Html.Partial("TimeSheetEmployeePartialView", employee)
        }
    </table>
        if (Model.IsFirst)
        {

@*<table class="paperNum" border="1" bordercolor='black' cellpadding="0" cellspacing="0" style='border-collapse: collapse; table-layout: fixed;'>
            </table>*@

        }
    @Html.Partial("ApproverListPartialView", Model.Approvers);
@*@Html.Partial("PaperFooter", Model)*@
    <div class="paperNum">
        @Html.Partial("PaperFooter", Model)
    </div>

}
else
{

    <table border="2" bordercolor="black" cellpadding="0" cellspacing="0" style="border-collapse: collapse; table-layout: fixed;">
        @Html.Partial("TimeSheetHeader", Model.HeaderStyle)
        @foreach (var employee in Model.Employees)
        {
            @Html.Partial("TimeSheetEmployeePartialView", employee)
        }
    </table>
    <div class="paperNum">
        @Html.Partial("PaperFooter", Model)
    </div>
}



